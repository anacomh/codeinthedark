{
  "name": "@ashthornton/asscroll",
  "version": "1.7.9",
  "description": "Ash's Smooth Scroll",
  "main": "build/asscroll.js",
  "unpkg": "build/asscroll.min.js",
  "files": [
    "src",
    "css",
    "build"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules",
    "postpublish": "git push origin master && git push origin --tags",
    "postversion": "npm publish --access public",
    "preversion": "npm run production && git add -A && git commit -m build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashthornton/asscroll.git"
  },
  "keywords": [
    "smooth",
    "scroll"
  ],
  "author": "Ash Thornton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashthornton/asscroll/issues"
  },
  "homepage": "https://github.com/ashthornton/asscroll#readme",
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@babel/preset-env": "7.5.4",
    "@babel/register": "7.4.4",
    "babel-loader": "^8.0.6",
    "core-js": "^3.1.4",
    "cross-env": "^7.0.2",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.2.1",
    "regenerator-runtime": "^0.13.2",
    "terser-webpack-plugin": "^3.0.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@unseenco/e": "^1.7.0"
  }
}
